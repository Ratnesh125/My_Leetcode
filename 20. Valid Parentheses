class Solution {
public:
    bool isValid(string s) {
        stack<int>st;
        for(int i=0;i<s.size();i++){
            if(s[i]=='('||s[i]=='{'||s[i]=='['){
                st.push(s[i]);
            }
            else{
            if(st.empty()) return false;
            int t=st.top();
            if((s[i]==')'&& t=='(')  ||(s[i]=='}'&& t=='{') || (s[i]==']'&& t=='[')){
                    st.pop();
                }
                else{return false;}
            }
        }
        if(st.empty()) return true;
        return false;
    }
};
